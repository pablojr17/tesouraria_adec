{
  "version": 2,
  "builds": [
    {
      "src": "src/api/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "s/rc/api/server.ts"
    }
  ]
}
